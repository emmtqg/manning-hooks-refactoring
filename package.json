{
  "name": "refactoring-to-react-hooks-base",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/user-event": "^7.1.2",
    "chart.js": "^2.9.4",
    "cross-env": "^7.0.3",
    "cypress": "8.1.0",
    "cypress-react-unit-test": "^4.17.2",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-error-boundary": "^3.1.3",
    "react-query": "^3.18.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "react-select": "^4.3.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "start": "REACT_APP_ENV=development react-scripts start",
    "start:test": "REACT_APP_ENV=test react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "e2e": "REACT_APP_ENV=test cypress open",
    "cypressr": "cypress run"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "coverageReporters": [
      "html"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "miragejs": "^0.1.41",
    "prop-types": "^15.7.2",
    "regenerator-runtime": "^0.13.9",
    "time-series-data-generator": "^0.1.5"
  },
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/env"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ]
  }
}
